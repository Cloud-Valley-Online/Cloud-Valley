{
  "name": "ralphjsmit/pest-plugin-livewire",
  "description": "A plugin for Pest to test Livewire forms.",
  "keywords": [
    "php",
    "framework",
    "pest",
    "unit",
    "test",
    "testing",
    "plugin"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.76|^9.0",
    "livewire/livewire": "^2.8",
    "pestphp/pest": "^1.17",
    "pestphp/pest-plugin": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "RalphJSmit\\PestPluginLivewire\\": "src/"
    },
    "files": [
      "src/Autoload.php",
      "src/Functions.php",
      "src/Expectations.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "RalphJSmit\\PestPluginLivewire\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.23|^7.0",
    "pestphp/pest-dev-tools": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {}
}
